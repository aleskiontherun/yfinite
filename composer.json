    "keywords": ["statemachine", "workflow", "state", "transition", "yii2"],
    "homepage": "https://github.com/dizeee/yfinite",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksei Vesnin",
            "email": "dizeee@dizeee.ru",
            "homepage": "https://github.com/dizeee"
        },
